{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["storage", "declarativeContent", "tabs"],
    "background": {
      "scripts": ["jquery-3.5.1.min.js", "background.js"],
      "persistent": false
    },
    "content_scripts":[
      {
        "matches":["<all_urls>"],
        "js": ["jquery-3.5.1.min.js", "content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "manifest_version": 2
}
